<div id="page-wrap">
  <div id="page0" style="height:500px"><a name="home"></a></div>


  <div id="page1">
    <div class="container">
      <a name="github"></a>

      <div class="page-padding"></div>
      <div class="col-md-6">
        <%= image_tag("github-logo.png", :alt => "github", :height => '430', :width => '340') %>
